  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "cypress": "^14.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "typescript": "^5.7.3"
  }
}
