{
  "name": "cypress-e2e-docs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "lint": "eslint . --ext .ts,.js",
    "test:dev": "cypress run --config-file cypress/config/dev.config.ts",
    "test:qa": "cypress run --config-file cypress/config/qa.config.ts",
    "test:stg": "cypress run --config-file cypress/config/stg.config.ts",
    "test:regression": "CYPRESS_TEST_SUITE=regression cypress run --config-file cypress/config/base.config.ts",
    "test:smoke": "CYPRESS_TEST_SUITE=smoke cypress run --config-file cypress/config/base.config.ts",
    "test:regression:dev": "CYPRESS_TEST_SUITE=regression cypress run --config-file cypress/config/dev.config.ts",
    "test:regression:qa": "CYPRESS_TEST_SUITE=regression cypress run --config-file cypress/config/qa.config.ts",
    "test:regression:stg": "CYPRESS_TEST_SUITE=regression cypress run --config-file cypress/config/stg.config.ts",
    "test:smoke:dev": "CYPRESS_TEST_SUITE=smoke cypress run --config-file cypress/config/dev.config.ts",
    "test:smoke:qa": "CYPRESS_TEST_SUITE=smoke cypress run --config-file cypress/config/qa.config.ts",
    "test:smoke:stg": "CYPRESS_TEST_SUITE=smoke cypress run --config-file cypress/config/stg.config.ts"
  },
  "keywords": [],
